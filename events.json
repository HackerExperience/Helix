{
  "handlers": {
    "Account.Account": {
      "receives": ["Account.Verified"],
      "emits": ["Entity.Created"]
    },
    "Bank.Account": {
      "receives": ["Bank.Account.Password.Reveal.Processed"],
      "emits": ["Bank.Account.Password.Revealed"]
    },
    "Bank.Transfer": {
      "receives": [
          "Bank.Transfer.Completed",
          "Bank.Transfer.Aborted"
      ],
      "emits": []
    },
    "Entity.Database": {
        "receives": [
            "Server.Password.Acquired",
            "Bank.Account.Password.Revealed",
            "Bank.Token.Acquired",
            "Bank.Account.Login"
        ],
        "emits": []
    },
    "Software.File.Transfer": {
        "receives": ["File.Transfer.Processed"],
        "emits": [
            "File.Downloaded",
            "File.DownloadFailed",
            "File.Uploaded",
            "File.UploadFailed"
        ]
    },
    "Software.Cracker": {
        "receives": [
            "Cracker.Bruteforce.Processed",
            "Cracker.Overflow.Processed"
        ],
        "emits": [
            "Server.Password.Acquired",
            "Cracker.Bruteforce.Failed",
            "Bank.Token.Acquired"
        ]
    },
    "Software.Filesystem": {
        "receives": [
            "File.Downloaded",
            "File.Uploaded"
        ],
        "emits": ["Filesystem.Changed"]
    },
    "Software.Virus": {
      "receives": [
        "File.Install.Processed"
      ],
      "emits": [
        "Virus.Installed",
        "Virus.InstallFailed"
      ]
    },
    "Log.Log": {
      "receives": [
        "File.Downloaded",
        "File.Uploaded",
        "Log.Forge.Edited",
        "Log.Forge.Created",
        "Bank.Transfer.Completed",
        "Connection.Started"
      ],
      "emits": [
        "Log.Created",
        "Log.Modified",
        "Log.Deleted"
      ]
    },
    "Process.Process":{
      "receives": [
        "Process.Signaled"
      ],
      "emits": [
        "Custom"
      ]
    },
    "Process.TOP": {
      "receives": [
        "Connection.Closed",
        "Process.Created",
        "TOP.BringMeToLife"
      ],
      "emits": [
        "TOP.Recalcado",
        "Process.Completed",
        "Process.Created",
        "Process.CreateFailed"
      ]
    },
    "Network.Connection": {
      "receives": [
        "Bank.Transfer.Completed"
      ],
      "emits": [
        "Connection.Closed"
      ]
    },
    "Tunnel.Connection": {
      "receives": ["Connection.Closed"],
      "emits": []
    },
    "Story.Manager": {
      "receives": ["Entity.Created"],
      "emits": ["Steppable (custom)"]
    },
    "Story.Story": {
      "receives": ["All"],
      "emits": [
        "Story.Step.Proceeded",
        "Story.Step.Failed",
        "Steppable (custom)"
      ]
    },
    "Notification": {
      "receives": ["All"],
      "emits": ["Notificable (custom)"]
    }
  },
  "flows": {
    "Account.Account": ["Entity.Created"],
    "Bank.Account": [
      "Bank.Account.Login",
      "Connection.Started"
    ],
    "Bank.Transfer": [
      "Connection.Started",
      "Process.Created"
    ],
    "Network.Bounce": [
      "Network.Bounce.Created",
      "Network.Bounce.CreateFailed",
      "Network.Bounce.Removed",
      "Network.Bounce.RemoveFailed",
      "Network.Bounce.Updated",
      "Network.Bounce.UpdateFailed"
    ],
    "Software.File.Transfer": [
      "Connection.Started",
      "Process.Created"
    ],
    "Software.Software.Cracker": [
      "Connection.Started",
      "Process.Created"
    ],
    "Software.Software.Firewall": [
      "Process.Created",
      "Firewall.Started"
    ],
    "Software.Software.LogForger": [
      "Process.Created"
    ],
    "Story.Manager": [
      "Story.Proceeded"
    ]
  },
  "process_conclusion": [
    "Bank.Account.Password.Reveal.Processed",
    "Cracker.Bruteforce.Processed",
    "Cracker.Overflow.Processed",
    "File.Install.Processed",
    "File.Transfer.Processed"
  ],
  "notificable": [
    "Log.Created",
    "Log.Modified",
    "Log.Deleted",
    "Network.Bounce.Created",
    "Network.Bounce.CreateFailed",
    "Network.Bounce.Removed",
    "Network.Bounce.RemoveFailed",
    "Network.Bounce.Updated",
    "Network.Bounce.UpdateFailed",
    "Server.Password.Acquired",
    "Process.Created",
    "Process.Completed",
    "Process.Killed",
    "File.Uploaded",
    "StoryEmailSentEvent",
    "StoryReplySentEvent",
    "Virus.Installed",
    "Virus.InstallFailed"
  ],
  "missions": {
    "Tutorial": {
      "steps": {
        "SetupPC": {
          "filters": ["Account.Created"],
          "emits": []
        },
        "DownloadCracker": {
          "filters": ["File.Downloaded"],
          "emits": []
        }
      }
    }
  }
}
